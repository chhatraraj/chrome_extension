{
    "manifest_version": 3,
    "name": "Productivity Booster",
    "version": "1.0",
    "description": "Boost your productivity by blocking distracting websites and providing motivational quotes.",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "permissions": [
      "tabs",
      "storage",
      "activeTab",
      "alarms"
    ],
    "background": {
      "service_worker": "background.js"
    },
    "host_permissions": [
      "<all_urls>"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icons/icon48.png"
    }
  }
  